<div class="container">
<h2>Edit profile</h2>
	<%= form_for @user do |form| %>
    <%= render partial: '/users/form', object: form %>

    <div class="submit-field">
      <%= form.submit class:"btn btn-success" %>
    </div>

  <% end %>
  <%= link_to "Cancel", @user, class: "btn btn-danger" %>
</div>